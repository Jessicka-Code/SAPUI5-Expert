{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run prueba2-HTML5Module",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/prueba2/prueba2-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/logali1HTML5Module/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/prueba2/HTML5Module",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/prueba2/HTML5Module\"}"
      },
      "envFile": "${workspaceFolder}/prueba2/HTML5Module/.env1",
      "preLaunchTask": "Build prueba2"
    },
    {
      "name": "Run Invoices-SAPUI5",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/Invoices/invoices-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/logaligroupSAPUI5/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/Invoices/SAPUI5",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Invoices/SAPUI5\"}"
      },
      "envFile": "${workspaceFolder}/Invoices/SAPUI5/.env7",
      "preLaunchTask": "Build Invoices"
    }
  ]
}